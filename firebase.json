{
  "hosting": [
    {
      "site": "fitos-88fff",
      "public": "dist/apps/landing/browser",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "function": "ssrApp"
        }
      ],
      "headers": [
        {
          "source": "**/*.@(jpg|jpeg|gif|png|svg|webp|js|css|woff|woff2|ttf|otf)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "max-age=31536000"
            }
          ]
        }
      ]
    },
    {
      "site": "fitos-mobile",
      "public": "dist/apps/mobile/browser",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ],
      "headers": [
        {
          "source": "**/*.@(jpg|jpeg|gif|png|svg|webp|js|css|woff|woff2|ttf|otf)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "max-age=31536000"
            }
          ]
        },
        {
          "source": "manifest.json",
          "headers": [
            {
              "key": "Content-Type",
              "value": "application/manifest+json"
            }
          ]
        }
      ]
    }
  ],
  "functions": {
    "source": "dist/apps/landing/server",
    "runtime": "nodejs22"
  }
}
