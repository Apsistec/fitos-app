    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.9.0",
    "typescript-eslint": "^8.40.0",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">=22.12.0"
  },
