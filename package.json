    "@angular/compiler-cli": "^21.1.1",
    "@angular/language-service": "^21.1.1",
    "@axe-core/cli": "^4.11.0",
    "@capacitor/cli": "^8.0.1",
    "@eslint/js": "^9.8.0",
    "@nx/angular": "^22.4.1",
    "@nx/eslint": "^22.4.1",
    "@nx/eslint-plugin": "^22.4.1",
    "@nx/jest": "^22.4.1",
    "@nx/js": "^22.4.1",
    "@nx/workspace": "^22.4.1",
    "@schematics/angular": "^21.1.1",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/canvas-confetti": "^1.9.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "18.16.9",
    "@typescript-eslint/utils": "^8.40.0",
    "@vitest/coverage-v8": "^3.0.0",
    "@vitest/ui": "^3.0.0",
    "angular-eslint": "^21.0.0",
    "autoprefixer": "^10.4.0",
    "axe-core": "^4.11.1",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "~14.6.1",
    "jest-util": "^29.7.0",
