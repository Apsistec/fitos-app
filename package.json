{
  "name": "fitos-app",
  "version": "0.1.0",
  "description": "FitOS - AI-Powered Fitness Coaching Platform",
  "private": true,
  "scripts": {
    "start": "nx serve fitos-mobile",
    "start:landing": "nx serve fitos-landing",
    "build": "nx build fitos-mobile",
    "build:landing": "nx build fitos-landing",
    "build:all": "nx run-many --target=build --all",
    "test": "nx test",
    "lint": "nx lint",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase migration up",
    "db:gen-types": "supabase gen types typescript --local > libs/shared/src/lib/database.types.ts",
    "ai:dev": "cd apps/ai-backend && poetry run python -m uvicorn main:app --reload"
