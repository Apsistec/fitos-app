{
  "name": "fitos-app",
  "version": "0.1.0",
  "description": "FitOS - AI-Powered Fitness Coaching Platform",
  "private": true,
  "scripts": {
    "start": "nx serve fitos-mobile",
    "start:landing": "nx serve fitos-landing",
    "build": "nx build fitos-mobile",
    "build:landing": "npm run changelog:generate && nx build fitos-landing",
    "build:all": "npm run changelog:generate && nx run-many --target=build --all",
    "test": "nx test",
    "lint": "nx lint",
    "changelog:generate": "node scripts/generate-changelog.js",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase migration up",
    "db:gen-types": "supabase gen types typescript --local > libs/shared/src/lib/database.types.ts",
    "ai:dev": "cd apps/ai-backend && poetry run python -m uvicorn main:app --reload",
    "deploy:landing": "npm run build:landing && firebase deploy --only hosting:fitos-88fff",
    "deploy:mobile:web": "npm run build && firebase deploy --only hosting:fitos-mobile",
    "deploy:both": "npm run build:all && firebase deploy --only hosting,functions",
    "deploy:preview:landing": "npm run build:landing && firebase hosting:channel:deploy preview --only fitos-88fff",
    "deploy:preview:mobile": "npm run build && firebase hosting:channel:deploy preview --only fitos-mobile",
    "deploy:mobile:android": "cd apps/mobile && npx cap sync android && cd android && ./gradlew assembleRelease",
    "deploy:mobile:ios": "cd apps/mobile && npx cap sync ios"
  },
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^2.2.2",
    "@angular-devkit/build-angular": "^21.1.3",
    "@angular-devkit/core": "^21.1.3",
    "@angular-devkit/schematics": "^21.1.3",
    "@angular/cli": "^21.1.3",
    "@angular/compiler-cli": "^21.1.3",
    "@angular/language-service": "^21.1.1",
    "@axe-core/cli": "^4.11.0",
    "@capacitor/cli": "^8.0.2",
    "@eslint/js": "^9.8.0",
    "@nx/angular": "^22.4.1",
    "@nx/eslint": "^22.4.1",
    "@nx/eslint-plugin": "^22.4.1",
    "@nx/js": "^22.4.1",
    "@nx/workspace": "^22.4.1",
    "@schematics/angular": "^21.1.3",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.15.11",
    "@swc/helpers": "~0.5.11",
    "@types/canvas-confetti": "^1.9.0",
    "@types/express": "^5.0.0",
    "@types/node": "25.2.2",
    "@typescript-eslint/utils": "^8.40.0",
    "@vitest/coverage-v8": "^3.0.0",
    "@vitest/ui": "^3.0.0",
    "angular-eslint": "^21.0.0",
    "autoprefixer": "^10.4.0",
    "axe-core": "^4.11.1",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "jsdom": "^28.0.0",
    "nx": "^22.4.1",
    "postcss": "^8.4.0",
    "prettier": "^2.6.2",
    "supabase": "^2.76.4",
    "tailwindcss": "^3.4.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.9.0",
    "typescript-eslint": "^8.40.0",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">=22.12.0"
  },
  "overrides": {
    "tar": "^7.5.7",
    "tmp": "^0.2.5",
    "xml2js": "^0.6.2"
  },
  "dependencies": {
    "@angular/animations": "^21.1.3",
    "@angular/cdk": "^21.1.1",
    "@angular/common": "^21.1.3",
    "@angular/compiler": "^21.1.3",
    "@angular/core": "^21.1.3",
    "@angular/forms": "^21.1.3",
    "@angular/platform-browser": "^21.1.3",
    "@angular/platform-browser-dynamic": "^21.1.3",
    "@angular/platform-server": "^21.1.1",
    "@angular/router": "^21.1.3",
    "@angular/service-worker": "^21.1.3",
    "@angular/ssr": "^21.1.1",
    "@capacitor/app": "^8.0.0",
    "@capacitor/core": "^8.0.2",
    "@capacitor/device": "^8.0.0",
    "@capacitor/haptics": "^8.0.0",
    "@capacitor/preferences": "^8.0.0",
    "@capacitor/status-bar": "^8.0.0",
    "@ionic/angular": "^8.7.17",
    "@sentry/angular": "^10.38.0",
    "@simplewebauthn/browser": "^13.2.2",
    "@types/compression": "^1.8.1",
    "canvas-confetti": "^1.9.4",
    "chart.js": "^4.5.1",
    "compression": "^1.8.1",
    "express": "^5.2.1",
    "firebase": "^12.9.0",
    "ng2-charts": "^8.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.8.0",
    "zone.js": "~0.15.0"
  }
}
