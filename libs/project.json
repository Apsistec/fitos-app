{
  "name": "shared-content",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": [],
  "targets": {
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest run --config libs/vitest.config.ts",
        "cwd": "{workspaceRoot}"
      },
      "configurations": {
        "ci": {
          "command": "vitest run --config libs/vitest.config.ts --reporter=default"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
